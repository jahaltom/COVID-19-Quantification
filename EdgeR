library(edgeR)



#Gene_name Gene_stable_ID COVSUBJ_0001_1_P_HA_all-reads COVSUBJ_0002_1_N_HA_all-read
rawdata <- read.delim("file", check.names=FALSE, stringsAsFactors=FALSE)
y <- DGEList(counts=rawdata[,3:4], genes=rawdata[,1:2])



rownames(y$counts) <- rownames(y$genes) <- y$genes$Gene_stable_ID

y <- calcNormFactors(y, method="TMM")
cpm(y) #Obtain the TMM values of your data
